<page>
	<view class="container">
		<block wx:for-items="{{jokeListInfo}}">
			<view style="background-color: #3d3d3b;height: 0.5px;"></view>
			<view class="joke_content">
				<!-- 笑话内容 -->
				<view>
					<text>{{item.content}}</text>
				</view>
				<!-- 笑话更新时间 -->
				<view style="text-align: right;font-size: 11px;padding-top: 10px;padding-right: 10px;padding-bottom: 3px;">
					<text>更新时间:{{item.updatetime}}</text>
				</view>
			</view>
			<view style="background-color: #3d3d3b;height: 0.5px;"></view>
			
		</block>
	</view>
</page>